import java.util.Scanner;
Class Prime {
  Public Static void main(String ag[]) {
    int n, p;
    Scanner s=new Scanner(System.in);
    System.out.println("Enter upto which number prime number needed.");
    n=s.nextInt();
    System.out.println("< Prime number ---->");
    for(int i=2; i<n; i++) {
      p=0;
      for(int j=2; j<i; j++) {
        if(i%j==0) {
          p=1;
        }
      }
      if (p==0) {
        System.out.println(i);
      }
    }
  }
}
